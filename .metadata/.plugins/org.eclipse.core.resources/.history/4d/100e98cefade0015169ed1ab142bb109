<?xml version="1.0" encoding="UTF-8"?>
<project name="Ejercicio1-xml" default="compile" basedir=".">
	<description>Ejemplo de fichero build.xml</description>
	<target name="delete" description="Borra los directorios">
		<delete dir="${basedir}/Build"/>
		<delete dir="${basedir}/Dist"/>
		<delete dir="${basedir}/Doc"/>
	</target>
	<target name="create" description="Crea los directorios">
		<create dir="${basedir}/Build"/>
		<create dir="${basedir}/Dist"/>
		<create dir="${basedir}/Doc"/>
	</target>
	<target name="compile" description="Compila"></target>
	<target name="distribute" description="Distribuye"></target>
	<target name="generateDoc" description="Genera el JavaDoc"></target>
</project>